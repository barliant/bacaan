@Article{Ma2004Rapidrecentgrowth,
  Title                    = {{Rapid recent growth and divergence of rice nuclear genomes}},
  Author                   = {Ma, Jianxin and Bennetzen, Jeffrey L},
  Journal                  = {PNAS},
  Year                     = {2004},
  Number                   = {34},
  Pages                    = {12404--12410},
  Volume                   = {101},

  __markedentry            = {[Anung:6]},
  Abstract                 = {By employing the nuclear DNA of the African rice as a reference genome, the timing, natures, mechanisms, and specificities of recent sequence evolution in the and subspecies of were identified. The data indicate that the genome sizes of both and have increased substantially, >2\% and >6\%, respectively, since their divergence from a common ancestor, mainly because of the amplification of LTR-retrotransposons. However, losses of all classes of DNA sequence through unequal homologous recombination and illegitimate recombination have attenuated the growth of the rice genome. Small deletions have been particularly frequent throughout the genome. In >1 Mb of orthologous regions that we analyzed, no cases of complete gene acquisition or loss from either or were found, nor was any example of precise transposon excision detected. The sequences between genes were observed to have a very high rate of divergence, indicating a molecular clock for transposable elements that is at least 2-fold more rapid than synonymous base substitutions within genes. We found that regions prone to frequent insertions and deletions also exhibit higher levels of point mutation. These results indicate a highly dynamic rice genome with competing processes for the generation and removal of genetic variation.},
  Doi                      = {10.1073/pnas.0403715101},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23},
  Url                      = {http://www.pnas.org/content/101/34/12404.abstract}
}

