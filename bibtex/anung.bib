@Article{Xing2007MobileDNAelements,
  Title                    = {{Mobile DNA elements in primate and human evolution}},
  Author                   = {Xing, Jinchuan and Witherspoon, David J and Ray, David A and Batzer, Mark A and Jorde, Lynn B},
