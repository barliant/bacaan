@Article{Reisenauer2013StochasticDynamicsLexicon,
  Title                    = {{Stochastic Dynamics of Lexicon Learning in an Uncertain and Nonuniform World}},
  Author                   = {Reisenauer, Rainer and Smith, Kenny and Blythe, Richard A.},
