@Article{Ammiraju2006Oryzabacterialartificial,
  Title                    = {{The Oryza bacterial artificial chromosome library resource: Construction and analysis of 12 deep-coverage large-insert BAC libraries that represent the 10 genome types of the genus Oryza}},
  Author                   = {Ammiraju, Jetty S S and Luo, Meizhong and Goicoechea, Jose L and Wang, Wenming and Kudrna, Dave and Mueller, Christopher and Talag, Jayson and Kim, HyeRan and Sisneros, Nicholas B and Blackmon, Barbara and Fang, Eric and Tomkins, Jeffery B and Brar, Darshan and MacKill, David and McCouch, Susan and Kurata, Nori and Lambert, Georgina and Galbraith, David W and Arumuganathan, K and Rao, Kiran and Walling, Jason G and Gill, Navdeep and Yu, Yeisoo and SanMiguel, Phillip and Soderlund, Carol and Jackson, Scott and Wing, Rod A},
