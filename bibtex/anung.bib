@Article{Klimek2011blogosphereasexcitable,
  Title                    = {{The blogosphere as an excitable social medium: Richterâ€™s and Omoriâ€™s Law in media coverage}},
  Author                   = {Klimek, Peter and Bayer, Werner and Thurner, Stefan},
  Journal                  = {Physica A: Statistical Mechanics and its Applications},
  Year                     = {2011},

  Month                    = oct,
  Number                   = {21-22},
  Pages                    = {3870--3875},
  Volume                   = {390},

  __markedentry            = {[Anung:6]},
  Abstract                 = {We study the dynamics of public media attention by monitoring the content of online blogs. Social and media events can be traced by the propagation of word frequencies of related keywords. Media events are classified as exogenousâ€“where blogging activity is triggered by an external news itemâ€“or endogenous where word frequencies build up within a blogging community without external influences. We show that word occurrences exhibit statistical similarities to earthquakes. Moreover the size distribution of events scales with a similar exponent as found in the Gutenbergâ€“Richter law. The dynamics of media events before and after the main event can be satisfactorily modeled as a type of process which has been used to understand foreâ€“and aftershock rate distributions in earthquakesâ€“the Omori law. We present empirical evidence that for media events of endogenous origin the overall public reception of the event is correlated with the behavior of word frequencies at the beginning of the event, and is to a certain degree predictable. These results imply that the process of opinion formation in a human society might be related to effects known from excitable media.},
  Doi                      = {10.1016/j.physa.2011.05.033},
  File                     = {::},
  ISSN                     = {03784371},
  Keywords                 = {empirical power laws,excitable social system,phys-lang,statistical human dynamics},
  Mendeley-tags            = {phys-lang},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23},
  Url                      = {http://dx.doi.org/10.1016/j.physa.2011.05.033 http://linkinghub.elsevier.com/retrieve/pii/S0378437111004390}
}

@Article{Knorr1996Findingaggregateproximity,
  Title                    = {{Finding aggregate proximity relationships and commonalities in spatial data mining}},
  Author                   = {Knorr, E M and Ng, R T},
  Journal                  = {Knowledge and Data Engineering, IEEE Transactions on},
  Year                     = {1996},
  Number                   = {6},
  Pages                    = {884--897},
  Volume                   = {8},

  __markedentry            = {[Anung:6]},
  Abstract                 = {Studies two spatial knowledge discovery problems involving proximity relationships between clusters and features. The first problem is: given a cluster of points, how can we efficiently find features (represented as polygons) that are closest to the majority of points in the cluster? We measure proximity in an aggregate sense due to the nonuniform distribution of points in a cluster (e.g. houses on a map), and the different shapes and sizes of features (e.g. natural or man-made geographic features). The second problem is: given n clusters of points, how can we extract the aggregate proximity commonalities (i.e. features) that apply to most, if not all, of the n clusters? Regarding the first problem, the main contribution of the paper is the development of Algorithm CRH (Circle, Rectangle and Hull), which uses geometric approximations (i.e. encompassing circles, isothetic rectangles and convex hulls) to filter and select features. The highly scalable and incremental Algorithm CRH can examine over 50,000 features and their spatial relationships with a given cluster in approximately one second of CPU time. Regarding the second problem, the key contribution is the development of Algorithm GenCom (Generalization for Commonality extraction) that makes use of concept generalization to effectively derive many meaningful commonalities that cannot be found otherwise},
  Annote                   = {1041-4347},
  Keywords                 = {Algorithm CRH,Algorithm GenCom,CPU time,aggregate proximity relationships,commonality extraction,computational geometry,concept generalization,convex hulls,deductive databases,encompassing circles,feature extraction,feature shapes,feature sizes,geographic information systems,geometric approximations,geometric filtering,isothetic rectangles,knowledge acquisition,nonuniform distribution,pattern recognition,point clusters,polygons,spatial data mining,spatial knowledge discovery problems,spatial reasoning,visual databases},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23}
}

