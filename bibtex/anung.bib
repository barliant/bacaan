@Article{Nicolaidis2009RandomMatrixApproach,
  Title                    = {{A Random Matrix Approach to Language Acquisition}},
  Author                   = {Nicolaidis, A and Kosmidis, Kosmas and Argyrakis, Panos},
  Journal                  = {arXiv},
  Year                     = {2009},

  Month                    = sep,
  Pages                    = {16},
  Volume                   = {physics.so},

  __markedentry            = {[Anung:6]},
  Abstract                 = {Since language is tied to cognition, we expect the linguistic structures to reflect patterns we encounter in nature and analyzed by physics. Within this realm we investigate the process of protolanguage acquisition, using analytical and tractable methods developed within physics. A protolanguage is a mapping between sounds and objects (or concepts) of the perceived world. This mapping is represented by a matrix and the linguistic interaction among individuals is described by a random matrix model. There are two essential parameters in our approach. The strength of the linguistic interaction \$\backslash beta\$, which following Chomsky's tradition, we consider as a genetically determined ability, and the number \$N\$ of employed sounds (the lexicon size). Our model of linguistic interaction is analytically studied using methods of statistical physics and simulated by Monte Carlo techniques. The analysis reveals an intricate relationship between the innate propensity for language acquisition \$\backslash beta\$ and the lexicon size \$N\$, \$N \backslash sim \backslash exp(\backslash beta)\$. Thus a small increase of the genetically determined \$\backslash beta\$ may lead to an incredible lexical explosion. Our approximate scheme offers an explanation for the biological affinity of different species and their simultaneous linguistic disparity.},
  Annote                   = {16 pages, 4 figures. Submitted to JSTAT},
  Archiveprefix            = {arXiv},
  Arxivid                  = {0909.3912},
  Eprint                   = {0909.3912},
  File                     = {::},
  Keywords                 = {Physics and Society,phys-lang},
  Mendeley-tags            = {phys-lang},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23},
  Url                      = {http://arxiv.org/abs/0909.3912v1 http://arxiv.org/abs/0909.3912}
}

