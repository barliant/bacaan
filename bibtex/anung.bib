@Article{Dorer1994Expansionstransgenerepeats,
  Title                    = {{Expansions of transgene repeats cause heterochromatin formation and gene silencing in Drosophila}},
  Author                   = {Dorer, D R and Henikoff, S},
  Journal                  = {Cell},
