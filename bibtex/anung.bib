@Article{Feschotte2005DNAbindingspecificity,
  Title                    = {{DNA-binding specificity of rice mariner-like transposases and interactions with Stowaway MITEs.}},
  Author                   = {Feschotte, C\'{e}dric and Osterlund, Mark T and Peeler, Ryan and Wessler, Susan R},
  Journal                  = {Nucleic acids research},
  Year                     = {2005},

  Month                    = jan,
  Number                   = {7},
  Pages                    = {2153--65},
  Volume                   = {33},

  __markedentry            = {[Anung:6]},
  Abstract                 = {Mariner-like elements (MLEs) are DNA transposons found throughout the plant and animal kingdoms. A previous computational survey of the rice (Oryza sativa) genome sequence revealed 34 full length MLEs (Osmars) belonging to 25 distinct families. This survey, which also identified sequence similarities between the Osmar elements and the Stowaway superfamily of MITEs, led to the formulation of a hypothesis whereby Stowaways are mobilized by OSMAR transposases. Here we investigate the DNA-binding activities and specificities of two OSMAR transposases, OSMAR5 and OSMAR10. Like other mariner-like transposases, the OSMARs bind specifically to the terminal inverted repeat (TIR) sequences of their encoding transposons. OSMAR5 binds DNA through a bipartite N-terminal domain containing two functionally separable helix-turn-helix motifs, resembling the paired domain of Tc1-like transposases and PAX transcription factors in metazoans. Furthermore, binding of the OSMARs is not limited to their own TIRs; OSMAR5 transposase can also interact in vitro with TIRs from closely related Osmar elements and with consensus TIRs of several Stowaway families mined from the rice genome sequence. These results provide the first biochemical evidence for a functional relationship between Osmar elements and Stowaway MITEs and lead us to suggest that there is extensive cross-talk among related but distinct transposon families co-existing in a single eukaryote genome.},
  Doi                      = {10.1093/nar/gki509},
  File                     = {::},
  ISSN                     = {1362-4962},
  Keywords                 = {Base Sequence,Binding Sites,DNA Mutational Analysis,DNA Transposable Elements,DNA-Binding Proteins,DNA-Binding Proteins: chemistry,DNA-Binding Proteins: genetics,DNA-Binding Proteins: metabolism,Oryza sativa,Oryza sativa: enzymology,Oryza sativa: genetics,Phylogeny,Protein Structure, Tertiary,Sequence Alignment,Transposases,Transposases: chemistry,Transposases: genetics,Transposases: metabolism},
  Owner                    = {Anung},
  Pmid                     = {15831788},
  Timestamp                = {2015.05.23},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1079968\&tool=pmcentrez\&rendertype=abstract}
}

