@Article{Zhang2006ComputationalidentificationmicroRNAs,
  Title                    = {{Computational identification of microRNAs and their targets}},
  Author                   = {Zhang, Baohong and Pan, Xiaoping and Wang, Qinglian and Cobb, George P and Anderson, Todd A},
