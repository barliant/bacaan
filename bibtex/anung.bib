  File                     = {::;::},
  Keywords                 = {fuzzy control,fuzzy logic,fuzzy rule base,fuzzy rule generation,input space,knowledge based systems,learning by example,learning from examples,linguistic rules,mapping,output space,time series prediction,truck backer-upper control},
