@Article{Chaisson2004Fragmentassemblywith,
  Title                    = {{Fragment assembly with short reads}},
  Author                   = {Chaisson, Mark and Pevzner, Pavel and Tang, Haixu},
  Journal                  = {Bioinformatics},
  Year                     = {2004},
