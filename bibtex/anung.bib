  Keywords                 = {knowledge extraction,linked data,multilingual knowledge bases,rdf,wikipedia},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23},
  Url                      = {http://svn.aksw.org/papers/2013/SWJ\_DBpedia/public.pdf}
}

