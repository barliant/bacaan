  Keywords                 = {Brain,Brain: metabolism,Cystine,Cystine: metabolism,Enzymes,Enzymes: metabolism,Homocysteine,Homocysteine: metabolism,Humans,Mental Disorders,Mental Disorders: metabolism,Metabolomics,Metabolomics: methods,Phenotype},
  Owner                    = {Anung},
  Pmid                     = {23870038},
