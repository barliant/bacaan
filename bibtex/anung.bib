}

@Article{Lue2010Zipfslawleads,
  Title                    = {{Zipf's law leads to Heaps' law: analyzing their relation in finite-size systems.}},
  Author                   = {L\"{u}, Linyuan and Zhang, Zi-Ke and Zhou, Tao},
  Journal                  = {PloS one},
  Year                     = {2010},

  Month                    = jan,
  Number                   = {12},
  Pages                    = {e14139},
  Volume                   = {5},

