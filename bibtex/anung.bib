@Article{Kessler2012Younameit,
  Title                    = {{You name it--memory and delay govern first name dynamics.}},
  Author                   = {Kessler, David A and Maruvka, Yosi E and Ouren, J\o ergen and Shnerb, Nadav M},
  Journal                  = {PloS one},
  Year                     = {2012},

  Month                    = jan,
  Number                   = {6},
  Pages                    = {e38790},
  Volume                   = {7},

  __markedentry            = {[Anung:6]},
  Abstract                 = {The adoption and abandonment of first names through time is a fascinating phenomenon that may shed light on social dynamics and the forces that determine cultural taste in general. Here we show that baby name dynamics is governed almost solely by deterministic forces, even though the emerging abundance statistics resembles the one obtained from a pure drift model. Exogenous events are shown to affect the name dynamics very rarely, and most of the year-to-year fluctuations around the deterministic trend may be attributed solely to demographic noise. We suggest that the rise and fall of a name reflect an "infection" process with delay and memory. The symmetry between adoption and abandonment speed emerges from our model without further assumptions.},
  Editor                   = {Ben-Jacob, Eshel},
  ISSN                     = {1932-6203},
  Keywords                 = {Anthropology,Biology,Community ecology,Community structure,Computational Biology,Computational sociology,Computer Science,Computer modeling,Cultural anthropology,Demography,Ecology,Human families,Interdisciplinary physics,Linguistic anthropology,Physics,Population biology,Population modeling,Research Article,Social and behavioral sciences,Social anthropology,Social networks,Social research,Sociology,Statistical mechanics,phys-lang},
  Mendeley-tags            = {phys-lang},
  Owner                    = {Anung},
