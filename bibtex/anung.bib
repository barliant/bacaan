  Author                   = {Ghanbarnejad, Fakhteh and Gerlach, Martin and Miotto, Jose M. and Altmann, Eduardo G.},
  Journal                  = {Journal of The Royal Society Interface},
  Year                     = {2014},

  Month                    = jun,
