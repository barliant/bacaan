@Article{Montemurro2011Universalentropyword,
  Title                    = {{Universal entropy of word ordering across linguistic families.}},
  Author                   = {Montemurro, Marcelo A and Zanette, Dami\'{a}n H},
  Journal                  = {PloS one},
  Year                     = {2011},

  Month                    = jan,
  Number                   = {5},
  Pages                    = {e19875},
  Volume                   = {6},

  __markedentry            = {[Anung:6]},
  Abstract                 = {The language faculty is probably the most distinctive feature of our species, and endows us with a unique ability to exchange highly structured information. In written language, information is encoded by the concatenation of basic symbols under grammatical and semantic constraints. As is also the case in other natural information carriers, the resulting symbolic sequences show a delicate balance between order and disorder. That balance is determined by the interplay between the diversity of symbols and by their specific ordering in the sequences. Here we used entropy to quantify the contribution of different organizational levels to the overall statistical structure of language.},
  Doi                      = {10.1371/journal.pone.0019875},
  File                     = {::},
  ISSN                     = {1932-6203},
  Keywords                 = {phys-lang},
  Mendeley-tags            = {phys-lang},
  Owner                    = {Anung},
  Pmid                     = {21603637},
  Timestamp                = {2015.05.23},
  Url                      = {http://www.plosone.org/article/info:doi/10.1371/journal.pone.0019875 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3094390\&tool=pmcentrez\&rendertype=abstract}
}

@Article{MONTEMURRO2010TOWARDSQUANTIFICATIONSEMANTIC,
  Title                    = {{TOWARDS THE QUANTIFICATION OF THE SEMANTIC INFORMATION ENCODED IN WRITTEN LANGUAGE}},
  Author                   = {MONTEMURRO, MARCELO A. and ZANETTE, DAMI\'{A}N H.},
  Journal                  = {Advances in Complex Systems},
  Year                     = {2010},

  Month                    = jul,
  Number                   = {02},
  Pages                    = {135},
  Volume                   = {13},

  __markedentry            = {[Anung:6]},
  Abstract                 = {Written language is a complex communication signal capable of conveying information encoded in the form of ordered sequences of words. Beyond the local order ruled by grammar, semantic and thematic structures affect long-range patterns in word usage. Here, we show that a direct application of information theory quantifies the relationship between the statistical distribution of words and the semantic content of the text. We show that there is a characteristic scale, roughly around a few thousand words, which establishes the typical size of the most informative segments in written language. Moreover, we find that the words whose contributions to the overall information is larger, are the ones more closely associated with the main subjects and topics of the text. This scenario can be explained by a model of word usage that assumes that words are distributed along the text in domains of a characteristic size where their frequency is higher than elsewhere. Our conclusions are based on the analysis of a large database of written language, diverse in subjects and styles, and thus are likely to be applicable to general language sequences encoding complex information.},
  Annote                   = {19 pages, 4 figures, typos corrected},
  Archiveprefix            = {arXiv},
  Arxivid                  = {0907.1558},
  Doi                      = {10.1142/S0219525910002530},
  Eprint                   = {0907.1558},
  File                     = {::},
  ISSN                     = {0219-5259},
  Keywords                 = {Computation and Language,Data Analysis,Physics and Society,Statistics and Probability,phys-lang},
  Mendeley-tags            = {phys-lang},
  Owner                    = {Anung},
  Timestamp                = {2015.05.23},
  Url                      = {http://arxiv.org/abs/0907.1558v2 http://arxiv.org/abs/0907.1558 http://www.worldscinet.com/acs/13/1302/S0219525910002530.html}
}

@Article{Montemurro2001Entropicanalysisrole,
