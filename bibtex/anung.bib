@Article{Matsuo2004Repetitiveelementsin,
  Title                    = {{Repetitive elements in the major histocompatibility complex (MHC) class I region of a teleost, medaka: identification of novel transposable elements}},
  Author                   = {Matsuo, Megumi Y and Nonaka, Masaru},
  Journal                  = {Mechanisms of Development},
  Year                     = {2004},
