@InCollection{Wong2008ProtectingPrivacyin,
  Title                    = {{Protecting Privacy in Incremental Maintenance for Distributed Association Rule Mining}},
  Author                   = {Wong, W and Cheung, David and Hung, Edward and Liu, Huan},
