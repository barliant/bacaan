@Article{Schifanella2010FolksinFolksonomies,
  Title                    = {{Folks in Folksonomies}},
  Author                   = {Schifanella, Rossano and Barrat, Alain and Cattuto, Ciro and Markines, Benjamin and Menczer, Filippo},
